<Window x:Class="hobbit_save_manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hobbit_save_manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cbxSaveCollections" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="190" SelectionChanged="cbxSaveCollections_SelectionChanged"/>
        <ComboBox x:Name="cbxSaves" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Width="190" SelectionChanged="cbxSaves_SelectionChanged"/>
    </Grid>
</Window>
